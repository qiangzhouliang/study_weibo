.container {
  padding: 40rpx 60rpx;
}

.container .textarea {
  font-size: 28rpx;
  width: 100%;
  height: 200rpx;
}
/* 选择照片样式 */
.container .image-list-group{
  padding-top: 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.image-list-group .image-group{
  margin-right: 5rpx;
  position: relative;
  margin-bottom: 5rpx;
}
/* 添加按钮样式 */
.image-list-group .add-image-btn {
  font-size: 200rpx;
  color: #a9a9a9;
  background-color: #f2f2f2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add-image-btn view{
  margin-top: -24rpx;
}
.image-list-group .image-group image{
  width: 100%;
  height: 100%;
}
.image-list-group .image-group .close-btn{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: -30rpx;
  top: -30rpx;
  background-size: 50rpx;
  background-repeat: no-repeat;
  z-index: 1;
  background-image: url("data:image/png;base64,");
}
/* 照片样式结束 */
.container .location-group {
  margin-top: 40rpx;
  border-top: 1px solid #e4e4e4;
  border-bottom: 1px solid #e4e4e4;
  padding: 30rpx 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.location-group .left-group {
  display: flex;
  font-size: 28rpx;
}

.location-group .left-group .icon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}

.location-group .arrow {
  width: 40rpx;
  height: 40rpx;
}
.submit-btn {
  margin-top: 40rpx;
}

